{
  "patch": "Architect Config v6.1",
  "description": "Primary configuration file for Leafs Dynasty AI Architect Suite",
  "requires": {
    "canon": "v4.1",
    "dynastymax": "v4.9",
    "elite_convergence": "v5.0",
    "gameplanner": "v5.1",
    "tradecontract": "v5.1",
    "systemintel": "v5.1",
    "analytics": "v5.2",
    "scouting": "v5.2.1"
  },
  "paths": {
    "root": "Architect/",
    "modules": "Architect/modules/",
    "lines": "Architect/lines/",
    "reports": "executive/reports/",
    "snapshots": "snapshots/",
    "logs": "executive/logs/"
  },
  "lines_pointer": "Architect/lines/NHL_Lines_2024-11-28.json",
  "ahl_lines_pointer": "Architect/lines/AHL_Lines_2024-11-28.json",
  "default_goalie_plan": {
    "starter": "Joseph Woll",
    "backup": "Anthony Stolarz",
    "pull_rule": "4GA_or_<.860_after_40"
  },
  "autos": {
    "enable_autosave": true,
    "autosave_interval_games": 3,
    "enable_biweekly_audit": true,
    "enable_finance_audit": true,
    "enable_pre_scout_autoprep": true
  },
  "integrations": {
    "bridge_scout_trade": "active",
    "analytics_overlay": "enabled",
    "bench_ai": "linked_to_ticker",
    "discipline_heatmap": "on",
    "scout_trade_sync_card": "on",
    "targets_panel": "on"
  },
  "governance": {
    "canon_lock": true,
    "integrity_threshold": 0.995,
    "rollback_points": 3,
    "fail_safe_enabled": true
  },
  "logging": {
    "log_level": "standard",
    "log_frequency": "matchup",
    "log_to_disk": true,
    "notify_on_warnings": true
  },
  "qa_tasks": [
    "integrity_rescan",
    "morale_reindex",
    "chemistry_refresh",
    "cache_flush",
    "scout_refresh",
    "needs_forecast_update"
  ],
  "meta": {
    "author": "Austin Olsen",
    "team": "Toronto Maple Leafs",
    "environment": "Leafs Dynasty AI",
    "last_updated": "2024-11-28",
    "verified": true
  }
}